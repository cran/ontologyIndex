bibentry(
	header="To cite ontologyIndex in publications use:",
	bibtype= "Article",
	title        = "{ontologyX}: a suite of R packages for working with ontological data",
	author       = c(
					as.person("Daniel Greene"),
		   as.person("Sylvia Richardson"),
				   as.person("Ernest Turro")),
	journal      = "Bioinformatics",
	year         = "2017",
	pages        = "btw763",
	publisher	= "{Oxford Univ Press}",

	textVersion  =
	  paste("Greene D, Richardson S and Turro E (2017).",
		  "ontologyX: a suite of R packages for working with ontological data.",
		  "Bioinformatics, pp. btw763.")
)
